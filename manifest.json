{
  "name": "Cowin slot availability",
  "description": "To know available slot",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["src/background.js","src/popup.js"]
  },
  "permissions": ["storage","*://cdn-api.co-vin.in/*","notifications","declarativeContent"],

  "page_action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}